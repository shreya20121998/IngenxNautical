<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <Dialog id="charterNumberDialog" title="Select Charter Number">
        <content>
            <List items="{bidData>/biddingData}">
                <StandardListItem title="{bidData>Charter No.}" />
            </List>
        </content>
        <endButton>
            <Button text="Cancel" press="onCancel" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
