
<mvc:View xmlns:tnt="sap.tnt" xmlns:cards="sap.f.cards"
xmlns:form="sap.ui.layout.form"
controllerName="com.ingenx.nauti.transactionvoyage.controller.VoyageDashboard"
xmlns:core="sap.ui.core" xmlns="sap.m"
xmlns:mvc="sap.ui.core.mvc" displayBlock="true">

    <Page id="page" class="page" title="Voyage Dashboard" >

        <GenericTile id="_IDGenGenericTile1" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
         header="Nautical" subheader="Change Voyage" press="onPressChangeVoyage" >
            <TileContent id="_IDGenTileContent1">
                <core:Icon class="icon" src="sap-icon://BusinessSuiteInAppSymbols/icon-vessel" />
            </TileContent>
            
        </GenericTile>

        <GenericTile id="_IDGenGenericTile2" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
         header="Nautical" subheader="Voyage Approval" press="onVoyageApproval" >
            <TileContent id="_IDGenTileContent2">
                <core:Icon class="icon" src="sap-icon://BusinessSuiteInAppSymbols/icon-vessel" />
            </TileContent>
        </GenericTile>

    </Page>
       
</mvc:View>